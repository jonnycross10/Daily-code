def newArray(oldArray):
  arrlen = len(oldArray) 
  arrprod = 1
  tempArray = oldArray
  for i in range(0,arrlen):
    arrprod = arrprod * oldArray[i]
  for i in range(0,arrlen):
    var = arrprod / oldArray[i]
    tempArray[i] = var
  return tempArray

#test cases
print(newArray([3,2,1]))
print(newArray([1,2,3]))
print(newArray([1,3,5,7,9]))
print(newArray([]))
